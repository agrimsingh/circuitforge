{
  "name": "circuitforge",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run --exclude '__tests__/integration/**'",
    "test:integration": "vitest run __tests__/integration/ --testTimeout=180000 --exclude '__tests__/integration/agent-sdk*'",
    "test:sdk": "tsx __tests__/integration/agent-sdk.test.mts",
    "test:all": "vitest run --exclude '__tests__/integration/agent-sdk*' --testTimeout=180000 && tsx __tests__/integration/agent-sdk.test.mts",
    "test:watch": "vitest",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.39",
    "@vercel/sandbox": "^1.5.0",
    "circuit-json-to-bom-csv": "^0.0.8",
    "circuit-json-to-gerber": "^0.0.47",
    "circuit-json-to-pnp-csv": "^0.0.7",
    "convex": "^1.31.7",
    "jszip": "^3.10.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-resizable-panels": "^4.6.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
